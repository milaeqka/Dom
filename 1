#include<iostream>

int main()
{
	int myarray[6][6], sum=0;
	
	  
	  for(int j=0; j<6; j++)
	  {
	  	 for(int i=0; i<3; i++)
	  	 {
	  	 	cin>> myarray[j][i];
		   }
	  }
	  for(int j=0; j<6; j++)
	  {
	  	 for(int i=0; i<6; i++)
	  	 {
	  	    if(i+j==6-1)
			  {
			    sum+=myarray[j][i];
			  	
			  }
			
		   }
	  }
	        cout<< sum;
	return 0;
}
